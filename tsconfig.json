{
    "compilerOptions": {
        "module": "ESNext",
        "moduleResolution": "nodenext",
        "target": "ESNext",
        // "sourceMap": true,
        "strict": true,
        "noImplicitAny": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "outDir": "_dist/tsc",
        "plugins": [{ "name": "typescript-plugin-css-modules" }],
    },
    "include": ["_src/**/*"],
}